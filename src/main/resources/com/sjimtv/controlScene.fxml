<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.sjimtv.control.ControlScene">
   <children>
      <Button layoutX="220.0" layoutY="266.0" mnemonicParsing="false" onAction="#playPauseClicked" text="Play / Pause" textAlignment="CENTER">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Slider fx:id="seekbar" blockIncrement="1.0" layoutX="50.0" layoutY="311.0" majorTickUnit="100.0" max="1000.0" minorTickCount="1" prefHeight="75.0" prefWidth="500.0" showTickMarks="true" />
      <Slider fx:id="volumebar" layoutX="513.0" layoutY="56.0" orientation="VERTICAL" prefHeight="200.0" prefWidth="75.0" showTickLabels="true" value="100" />
      <ListView fx:id="showList" layoutX="60.0" layoutY="48.0" prefHeight="200.0" prefWidth="200.0" />
      <Button layoutX="274.0" layoutY="56.0" mnemonicParsing="false" text="Rebuild" onAction="#rebuildShow" />
   </children>
</AnchorPane>
