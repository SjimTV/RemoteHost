   <children>
      <Button layoutX="220.0" layoutY="175.0" mnemonicParsing="false" onAction="#playPauseClicked" text="Play / Pause" textAlignment="CENTER">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Slider fx:id="seekbar" blockIncrement="1.0" layoutX="50.0" layoutY="311.0" majorTickUnit="100.0" max="1000.0" minorTickCount="1" prefHeight="75.0" prefWidth="500.0" showTickMarks="true" />
      <Slider fx:id="volumebar" layoutX="513.0" layoutY="56.0"  orientation="VERTICAL" prefHeight="200.0" prefWidth="75.0" showTickLabels="true" value="100" />
   </children>
</AnchorPane>
